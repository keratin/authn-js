  "types": "./dist/index.d.ts",
  "author": "Lance Ivy",
  "license": "LGPL-3.0",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "jest": "^27.2.5",
    "msw": "^0.35.0",
    "prettier": "^2.4.1",
    "rollup": "^2.58.0",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-uglify": "^6.0.4",
